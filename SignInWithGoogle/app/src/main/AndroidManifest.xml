        android:icon="@mipmap/ic_launcher"
        android:label="뚜벅이"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:windowSoftInputMode="stateAlwaysHidden">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddPlan" />
        <activity android:name=".Checklist" />
        <activity android:name=".PlanDetail" />
        <activity android:name=".SetDetail" />
        <activity android:name=".Home" />
        <activity
            android:name=".fragment_Recommend"
            android:label="@string/title_activity_fragment__search_result"
            android:theme="@style/AppTheme" />
        <activity android:name=".fragment_recom_cultural" />
        <activity android:name=".fragment_recom_event" />
        <activity android:name=".fragment_recom_course" />
        <activity android:name=".fragment_recom_leports" />
        <activity android:name=".fragment_recom_accom" />
        <activity android:name=".fragment_recom_shopping" />
        <activity android:name=".fragment_recom_food"></activity>
